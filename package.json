{
  "name": "chorus-web-ui",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm exec tailwindcss -i src/styles/globals.css -o src/app/build.css",
    "build": "next build",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --turbo",
    "lint": "next lint --fix",
    "start": "next start",
    "test": "jest --watch",
    "test:run": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:visual": "jest --selectProjects visual",
    "test:a11y": "jest --selectProjects accessibility",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "format:all": "prettier --write .",
    "fix": "pnpm exec eslint . --fix && pnpm format:all && pnpm lint",
    "prepare": "simple-git-hooks || true"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run fix && pnpm test:run"
  },
  "dependencies": {
