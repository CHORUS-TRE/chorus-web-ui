  "engines": {
    "node": ">=22.14.0",
    "pnpm": "10.x",
    "npm": "please use pnpm",
    "yarn": "please use pnpm"
  },
  "pnpm": {
    "patchedDependencies": {
      "nextstepjs": "patches/nextstepjs.patch"
    }
  }
